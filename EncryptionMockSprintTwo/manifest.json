{
  "manifest_version": 2,
  "name": "Encryption Mock",
  "description": "This extension is a Encryption mockup",
  "version": "0.2.0",
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": ["activeTab", "storage"],
  "content_scripts":[
    {
      "matches": ["<all_urls>"],
      "css": ["css/injected.css"],
      "js": ["js/jquery-2.0.3.min.js", "js/jquery-ui.js",
              "js/cryptico/cryptico.js", "js/injected.js"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup/popup.html"
  },
  "web_accessible_resources" : ["js/jquery-2.0.3.min.map"]
}